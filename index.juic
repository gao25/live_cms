<!DOCTYPE HTML>
<html>
<head>
  <meta charset="utf-8">
  <title>CMS内容管理平台</title>
  <link rel="stylesheet" type="text/css" href="${{staticServer}}/css/base.css">
  <link rel="stylesheet" href="../static/css/common.css">
  <link rel="stylesheet" href="static/css/edit.css">
  <link rel="stylesheet" type="text/css" href="static/css/index.css">
  <script src="${{staticServer}}/js/jquery-1.11.0-min.js"></script>
</head>
<body>
<div class="lvs-head">
  <div class="logo"></div>
  <div id="j-nav" class="nav">
  </div>
  <div id="j-user" class="user">
    <div class="headpic"></div>
    <span class="uname"></span>
    <ul>
      <li class="lset"><a href="#">设置</a>
        <ul><li class="userset"><a href="#">个人设置</a></li></ul>
      </li> / 
      <li><a id="j-logout" href="javascript:void(0)">退出</a></li>
    </ul>
  </div>
</div>
<div class="lvs-nav">
  <div id="j-admin" class="user">
    <div class="headpic"></div>
    <span class="point"></span>
    <span class="uname"></span>
  </div>
  <div id="j-leftnav"></div>
</div>
<div class="lvs-iframe">
  <iframe scrolling="auto" frameborder="0" name="mainframe" src="about:blank"></iframe>
</div>
<div id="j-useroverly" class="fn-hide">
  <div class="lvs-overlay lvs-useroverlay">
    <div class="title">
      <span>个人信息</span><em class="close">X</em>
    </div>
    <iframe frameborder="0" scrolling="auto" width="600" height="400" name="setupuserFrame" src="about:blank"></iframe>
  </div>
</div>
<div class="fn-hide"><iframe id="j-executeframe" name="executeframe" src="about:blank"></iframe></div>
<script src="${{staticServer}}/js/cake/tplform/tplform-1.0.1.js"></script>
<script src="${{staticServer}}/js/cake/overlay/overlay-1.0.0.js"></script>
